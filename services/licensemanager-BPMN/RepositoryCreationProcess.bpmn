<tr><td><b>License</b></td><td>${mailData[7]}</td></tr>
<tr><td><b>Team</b></td><td>${mailData[8]}</td></tr>
<tr><td><b>Organization</b></td><td>${mailData[9]}</td></tr>
<tr><td><b>Repository Type</b></td><td>${mailData[10]}</td></tr>
<tr><td><b>Requested By</b></td><td>${mailData[11]}</td></tr>
<tr><td style="vertical-align:top;"><b>Description</b></td><td>${mailData[5].replaceAll("\\\\\\\\n","<br/>&emsp;").replaceAll("\\\\\\\\;",";")}</td></tr><br/>
</table>

<br/><br/>
Thank You.
</p>]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow27" name="Reject" sourceRef="exclusivegateway3" targetRef="mailtask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${outputType.equals( "Reject")}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow28" sourceRef="mailtask2" targetRef="endevent1"></sequenceFlow>
